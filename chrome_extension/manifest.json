{
  "name": "Lucid",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Barebones. Feeds the current tab url into the Amazon scraper.",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Lucid"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}