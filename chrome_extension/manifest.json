{
  "name": "Lucid",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Barebones. Feeds the current tab url into the Amazon scraper.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Lucid"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": [ "jquery-2.1.4.js" ]
  }],
  "content_security_policy": "script-src 'self' https://localhost:8080/amazon_prod/testing; object-src 'self'"
}
